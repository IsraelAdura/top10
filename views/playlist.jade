extends layout

block content
    h1 Playlist Parsing
    
    form#formAddUser(name="showplaylist",method="post",action="/getplaylist")
        input#inputPlaylist(type="text", placeholder="playlist slug", name="slug")

  
        button#btnSubmit(type="submit") submit

    ul
    
    for track in mydata
        div.songobject
            span.title #{track.title}
            span.artist #{track.artist.name} 
            span.url #{track.url} 
            span.thumb #{track.thumbnail}


